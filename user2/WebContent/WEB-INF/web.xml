  	<servlet-class>com.user2.controller.LoginCl</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginCl</servlet-name>
  	<url-pattern>/LoginCl</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>UserlistCl</servlet-name>
  	<servlet-class>com.user2.controller.UserlistCl</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UserlistCl</servlet-name>
